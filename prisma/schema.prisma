generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

// M = media
// P = potencia
// T = total
// Max = maximo/maxima

model DadosContratoComercial {
  id                   Int @id @default(autoincrement())
  Name                 String
  Email                String

  IdPainelName         String
  MConsumo             Float
  MIrradiacao          Float
  PModulos             Float
  AreaTInstalacao      Float
  TModulosAplicados    Int
  PSistema             Float
  PInversor            Float
  PTMaxSistemaKWP      Float
  created_at           DateTime @default(now())

  @@map("dadosContratoComercial")
}